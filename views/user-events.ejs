<% include partials/header %>
<% include partials/navBarStyle %>
<% include partials/adminNavbar %>
<% include partials/lineGaps %>

	<section class = "container">
		<h3> List of Events</h3>


		<table class="table table-hover">
		  <thead>
		    <tr>
		      <th scope="col">Event <br> Name</th>
		      <th scope="col">Starting Date <br> (yy-mm-dd) </th>
		      <th scope="col">Ending Date <br> (yy-mm-dd)</th>
		    </tr>
		  </thead>
		  <tbody>

					<% events.forEach((e)=>{%>
							<tr class="table-active">
								<td><a href="/ema/adminWelcome/events/show/<%= e.id %>/options"> <%=e.name%> </a></td>
								<td><%=e.start_date%></td>
								<td><%=e.end_date%></td>
							</tr>
					<%})%>

		  </tbody>
		</table>




		<% include partials/navbarScript %>

		<% include partials/footer %>
